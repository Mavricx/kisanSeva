      .social-links {
        display: flex;
        gap: 1.5rem;
      }

      .social-links img {
        width: 1.5rem;
        height: 1.5rem;
        opacity: 0.7;
        transition: opacity 0.3s ease;
      }

      .social-links img:hover {
        opacity: 1;
      }

      .footer-bottom {
        padding-top: 2rem;
        border-top: 1px solid rgba(255, 255, 255, 0.1);
        text-align: center;
        color: #9ca3af;
      }
    </style>
